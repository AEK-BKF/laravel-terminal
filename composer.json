{
    "name": "recca0120/terminal",
    "description": "run laravel artisan command in web application",
    "keywords": ["laravel", "artisan", "terminal", "console", "web", "web artisan"],
    "license": "MIT",
    "authors": [
        {
            "name": "recca0120",
            "email": "recca0120@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/console": "~5.2",
        "illuminate/database": "~5.2",
        "illuminate/filesystem": "~5.2",
        "illuminate/routing": "~5.2",
        "symfony/finder": "2.7.*|2.8.*|3.0.*|3.1.*"
    },
    "require-dev": {
        "mockery/mockery": "~0.9.4",
        "phpunit/phpunit": "~4.8|~5.4",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Recca0120\\Terminal\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.6.x-dev",
            "dev-laravel50": "1.4.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
